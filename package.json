{
  "name": "brainhack_western_website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "schema-gen": "npx typescript-json-schema ./tsconfig.json SiteConfig --required -o public/schemas/brainhack-config.json",
    "lint": "next lint",
    "analyze": "cross-env ANALYZE=true next build",
    "gh-build": "rm -rf node_modules/.cache && next build && next export && touch out/.nojekyll",
    "deploy": "npm run gh-build && git stash && git add -f out && git commit -m 'Deploying' && git subtree split --prefix out -b gh-pages && git push -f origin gh-pages:gh-pages && git branch -D gh-pages && git reset HEAD~1 && git stash pop && rm -rf out",
    "preinstall": "python3 --version | grep -Eq '[\\w\\s]*3\\.(8|9|10)\\.[0-9]+' || (echo 'Must have python 3.8, 3.9, or 3.10 installed and active' && exit 1)",
    "postinstall": "([ -d .venv ] || python3 -m venv .venv) && .venv/bin/python -m pip install git+https://github.com/pvandyken/overlapping-events.git"
  },
  "dependencies": {
    "@faire/mjml-react": "^3.3.0",
    "@tylermenezes/cognitoforms-react": "2.1.0",
    "@types/html-minifier-terser": "^7.0.0",
    "@types/luxon": "^3.3.1",
    "@types/mjml": "^4.7.1",
    "@types/mjml-core": "^4.7.1",
    "@types/node": "^20.5.7",
    "@types/react-helmet": "^6.1.6",
    "@vanilla-extract/css": "^1.9.1",
    "@vanilla-extract/sprinkles": "^1.5.0",
    "bootstrap": "^5.1.3",
    "color-interpolate": "^1.0.5",
    "color2k": "^2.0.0",
    "email-validator": "^2.0.4",
    "html-minifier-terser": "^7.2.0",
    "hypertag": "^0.0.6",
    "image-size": "^1.0.2",
    "immer": "^10.0.2",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "luxon": "^3.4.1",
    "mjml": "^4.14.1",
    "next": "^13.4.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-markdown": "^8.0.7",
    "react-obfuscate": "^3.6.8",
    "react-transition-group": "^4.4.5",
    "remark-gfm": "^3.0.0",
    "sass": "^1.42.1",
    "ts-debounce": "^4.0.0",
    "ts-functional-pipe": "^3.1.2",
    "word-join": "^0.0.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.0.1",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/bootstrap": "^5.1.6",
    "@types/color": "^3.0.2",
    "@types/js-yaml": "^4.0.3",
    "@types/lodash": "^4.14.183",
    "@types/react": "^18.2.21",
    "@types/react-transition-group": "^4.4.5",
    "@vanilla-extract/next-plugin": "^2.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.47.0",
    "eslint-config-next": "^13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.2",
    "typescript": "^5.2.2",
    "typescript-json-schema": "^0.60.0"
  }
}
