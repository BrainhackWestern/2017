{
  "name": "brainhack_western_website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "gh-build": "rm -rf node_modules/.cache && next build && next export && touch out/.nojekyll",
    "deploy": "npm run gh-build && git subtree split --prefix out -b gh-pages && git push -f origin gh-pages:gh-pages && git branch -D gh-pages && rm -rf out"
  },
  "dependencies": {
    "bootstrap": "^5.1.1",
    "js-yaml": "^4.1.0",
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-obfuscate": "^3.6.8",
    "sass": "^1.42.1",
    "ts-debounce": "^3.0.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.3",
    "@types/react": "17.0.27",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "typescript": "4.4.3"
  }
}
